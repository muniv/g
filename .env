# K-Intelligence 환경 설정

# API 서버 설정
REACT_APP_API_URL=http://localhost:51037
REACT_APP_API_TIMEOUT=30000

# 개발 모드 설정
REACT_APP_ENV=development
REACT_APP_DEBUG=true

# 파일 업로드 설정
REACT_APP_MAX_FILE_SIZE_IMAGE=10485760
REACT_APP_MAX_FILE_SIZE_DOCUMENT=52428800
REACT_APP_MAX_FILE_SIZE_TEXT=1048576

# 지원되는 파일 형식
REACT_APP_SUPPORTED_IMAGE_TYPES=image/jpeg,image/png,image/gif,image/webp
REACT_APP_SUPPORTED_DOCUMENT_TYPES=application/pdf,application/msword,application/vnd.openxmlformats-officedocument.wordprocessingml.document
REACT_APP_SUPPORTED_TEXT_TYPES=text/plain,text/markdown

# 기능 플래그
REACT_APP_ENABLE_HWP_SUPPORT=true
REACT_APP_ENABLE_IMAGE_PROCESSING=true
REACT_APP_ENABLE_QUESTION_GENERATION=true
REACT_APP_ENABLE_SEARCH=true

# UI 설정
REACT_APP_DEFAULT_LANGUAGE=ko
REACT_APP_ENABLE_DARK_MODE=true
REACT_APP_ENABLE_NOTIFICATIONS=true

# 성능 설정
REACT_APP_QUESTION_GENERATION_TIMEOUT=45000
REACT_APP_IMAGE_PROCESSING_TIMEOUT=30000
REACT_APP_DOCUMENT_PARSING_TIMEOUT=60000

# Analytics 설정 (선택사항)
# REACT_APP_GOOGLE_ANALYTICS_ID=
# REACT_APP_SENTRY_DSN=

# Ngrok 설정 (개발용)
# REACT_APP_NGROK_URL=https://your-ngrok-url.ngrok-free.app

# React Fast Refresh 비활성화 (WebSocket 에러 방지)
FAST_REFRESH=false